{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "version": "1.0.0-beta.28.3",
    "name": "mytb"
  },
  "apps": [
    {
      "name": "frontend",
      "platform": "browser",
      "root": "src/frontend",
      "outDir": "dist/browser/",
      "assets": [
        "assets",
        "manifest.json"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../../node_modules/ng2-toastr/bundles/ng2-toastr.min.css",
        "../../node_modules/leaflet/dist/leaflet.css",
        "../../node_modules/leaflet.markercluster/dist/MarkerCluster.css",
        "../../node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
        "../..//node_modules/angular2-lightbox/lightbox.css",
        "spinner.css",
        "responsive.css",
        "tables.css",
        "global.css",
        "global-overrides.css",
        "print.css",
        "print-overrides.css"
      ],
      "scripts": [],
      "serviceWorker": "true",
      "environmentSource": "environments/environment.ts",
      "environments": {
            "beta": "environments/environment.beta.ts",
            "dev": "environments/environment.ts",
            "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./frontend-protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/frontend/tsconfig.app.json"
    },
    {
      "project": "src/frontend/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./frontend-karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "build": {
      "preserveSymlinks": true
    },
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
