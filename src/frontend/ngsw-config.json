{
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html"
                ],
                "versionedFiles": [
                    "/*.bundle.css",
                    "/*.bundle.js",
                    "/*.chunk.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/*.jpg",
                    "/*.gif",
                    "/assets/**"
                ],
                "urls": [
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "dev",
            "urls": [
                "http://localhost:4200/**"
            ],
            "cacheConfig": {
                "maxSize": 100,
                "maxAge": "1d",
                "strategy": "freshness"
            }
        },
        {
            "name": "prod",
            "urls": [
                "http://localhost:43*/**",
                "http://www.michas-ausflugstipps.de/**",
                "http://www.stein-im-schuh.de/**"
            ],
            "cacheConfig": {
                "maxSize": 100,
                "maxAge": "1d",
                "strategy": "performance"
            }
        }
    ]
}
