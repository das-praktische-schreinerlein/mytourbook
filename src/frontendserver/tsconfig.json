{
  "compilerOptions": {
    "allowJs": true,
    "sourceMap": false,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "lib": [
      "es2016",
      "dom"
    ],
    "outDir": "../../dist/tsc-out-frontent",
    "module": "es2015",
    "baseUrl": "",
    "types": ["node"]
  },
  "angularCompilerOptions": {
    "preserveWhitespaces": true
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "node_modules",
    "dist"
  ]
}
