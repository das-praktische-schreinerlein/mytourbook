{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "frontend": {
            "root": "src",
            "sourceRoot": "src/frontend",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/browser/",
                        "tsConfig": "src/frontend/tsconfig.app.json",
                        "preserveSymlinks": true,
                        "polyfills": "src/frontend/polyfills.ts",
                        "assets": [
                            "src/frontend/assets",
                            "src/frontend/manifest.json"
                        ],
                        "styles": [
                            "node_modules/bootstrap/dist/css/bootstrap.min.css",
                            "node_modules/ngx-toastr/toastr.css",
                            "node_modules/leaflet/dist/leaflet.css",
                            "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
                            "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
                            "node_modules/ngx-lightbox/lightbox.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/browser-overrides.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/bootstrap-overrides.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/spinner.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/tables.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/base-layout.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/responsive.css",
                            "src/frontend/global.css",
                            "src/frontend/global-overrides.css",
                            "src/frontend/responsive.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/print.css",
                            "src/frontend/print.css",
                            "src/frontend/print-overrides.css"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "de": {
                            "i18nFile": "src/frontend/locales/messages.de.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "de",
                            "index": "src/frontend/index.html",
                            "main": "src/frontend/main.dev.ts"
                        },
                        "en": {
                            "i18nFile": "src/frontend/locales/messages.en.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "en",
                            "index": "src/frontend/index.html",
                            "main": "src/frontend/main.dev.ts"
                        },
                        "dev-de": {
                            "progress":  false,
                            "i18nFile": "src/frontend/locales/messages.de.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "de",
                            "index": "src/frontend/index.html",
                            "main": "src/frontend/main.dev.ts",
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "serviceWorker": true,
                            "ngswConfigPath": "src/frontend/ngsw-config.json"
                        },
                        "dev-en": {
                            "i18nFile": "src/frontend/locales/messages.en.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "en",
                            "index": "src/frontend/index.html",
                            "main": "src/frontend/main.dev.ts",
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "serviceWorker": true,
                            "ngswConfigPath": "src/frontend/ngsw-config.json"
                        },
                        "beta-de": {
                            "i18nFile": "src/frontend/locales/messages.de.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "de",
                            "index": "src/frontend/index.html",
                            "main": "src/frontend/main.beta.ts",
                            "fileReplacements": [
                                {
                                    "replace": "src/frontend/environments/environment.ts",
                                    "with": "src/frontend/environments/environment.beta.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "serviceWorker": true,
                            "ngswConfigPath": "src/frontend/ngsw-config.json"
                        },
                        "beta-en": {
                            "i18nFile": "src/frontend/locales/messages.en.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "en",
                            "index": "src/frontend/index.html",
                            "main": "src/frontend/main.beta.ts",
                            "fileReplacements": [
                                {
                                    "replace": "src/frontend/environments/environment.ts",
                                    "with": "src/frontend/environments/environment.beta.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "serviceWorker": true,
                            "ngswConfigPath": "src/frontend/ngsw-config.json"
                        },
                        "prod-de": {
                            "i18nFile": "src/frontend/locales/messages.de.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "de",
                            "index": "src/frontend/index.html",
                            "main": "src/frontend/main.prod.ts",
                            "fileReplacements": [
                                {
                                    "replace": "src/frontend/environments/environment.ts",
                                    "with": "src/frontend/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "serviceWorker": true,
                            "ngswConfigPath": "src/frontend/ngsw-config.json"
                        },
                        "prod-en": {
                            "i18nFile": "src/frontend/locales/messages.en.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "en",
                            "index": "src/frontend/index.html",
                            "main": "src/frontend/main.prod.ts",
                            "fileReplacements": [
                                {
                                    "replace": "src/frontend/environments/environment.ts",
                                    "with": "src/frontend/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "serviceWorker": true,
                            "ngswConfigPath": "src/frontend/ngsw-config.json"
                        },
                        "viewer-de": {
                            "i18nFile": "src/frontend/locales/messages.de.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "de",
                            "index": "src/frontend/index.viewer.html",
                            "main": "src/frontend/main.viewer.ts",
                            "fileReplacements": [
                                {
                                    "replace": "src/frontend/environments/environment.ts",
                                    "with": "src/frontend/environments/environment.viewer.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "serviceWorker": true,
                            "ngswConfigPath": "src/frontend/ngsw-config.json"
                        },
                        "viewer-en": {
                            "i18nFile": "src/frontend/locales/messages.en.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "en",
                            "index": "src/frontend/index.viewer.html",
                            "main": "src/frontend/main.viewer.ts",
                            "fileReplacements": [
                                {
                                    "replace": "src/frontend/environments/environment.ts",
                                    "with": "src/frontend/environments/environment.viewer.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "serviceWorker": true,
                            "ngswConfigPath": "src/frontend/ngsw-config.json"
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "frontend:build"
                    },
                    "configurations": {
                        "de": {
                            "browserTarget": "frontend:build:de"
                        },
                        "en": {
                            "browserTarget": "frontend:build:en"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "frontend:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/frontend/test.ts",
                        "karmaConfig": "./frontend-karma.conf.js",
                        "polyfills": "src/frontend/polyfills.ts",
                        "tsConfig": "src/frontend/tsconfig.spec.json",
                        "scripts": [],
                        "styles": [
                            "node_modules/bootstrap/dist/css/bootstrap.min.css",
                            "node_modules/ngx-toastr/toastr.css",
                            "node_modules/leaflet/dist/leaflet.css",
                            "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
                            "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
                            "node_modules/ngx-lightbox/lightbox.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/browser-overrides.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/bootstrap-overrides.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/spinner.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/tables.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/base-layout.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/responsive.css",
                            "src/frontend/global.css",
                            "src/frontend/global-overrides.css",
                            "src/frontend/responsive.css",
                            "node_modules/@dps/mycms-frontend-commons/dist/frontend-layout-commons/styles/print.css",
                            "src/frontend/print.css",
                            "src/frontend/print-overrides.css"
                        ],
                        "assets": [
                            "src/frontend/assets",
                            "src/frontend/manifest.json"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/frontend/tsconfig.app.json",
                            "src/frontend/tsconfig.spec.json"
                        ],
                        "exclude": []
                    }
                }
            }
        },
        "server": {
            "root": "src",
            "sourceRoot": "src/frontend",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:server",
                    "options": {
                        "outputPath": "dist/server",
                        "main": "src/frontend/main.server.ts",
                        "tsConfig": "src/frontend/tsconfig.server.json",
                        "preserveSymlinks": true,
                        "bundleDependencies": "all"
                    },
                    "configurations": {
                        "de": {
                            "i18nFile": "src/frontend/locales/messages.de.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "de"
                        },
                        "en": {
                            "i18nFile": "src/frontend/locales/messages.en.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "en"
                        },
                        "dev-de": {
                            "progress":  false,
                            "i18nFile": "src/frontend/locales/messages.de.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "de",
                            "optimization": false,
                            "outputHashing": "none",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false
                        },
                        "dev-en": {
                            "i18nFile": "src/frontend/locales/messages.en.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "en",
                            "optimization": false,
                            "outputHashing": "none",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false
                        },
                        "beta-de": {
                            "i18nFile": "src/frontend/locales/messages.de.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "de",
                            "fileReplacements": [
                                {
                                    "replace": "src/frontend/environments/environment.ts",
                                    "with": "src/frontend/environments/environment.beta.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "none",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false
                        },
                        "beta-en": {
                            "i18nFile": "src/frontend/locales/messages.en.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "en",
                            "fileReplacements": [
                                {
                                    "replace": "src/frontend/environments/environment.ts",
                                    "with": "src/frontend/environments/environment.beta.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "none",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false
                        },
                        "prod-de": {
                            "i18nFile": "src/frontend/locales/messages.de.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "de",
                            "fileReplacements": [
                                {
                                    "replace": "src/frontend/environments/environment.ts",
                                    "with": "src/frontend/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "none",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false
                        },
                        "prod-en": {
                            "i18nFile": "src/frontend/locales/messages.en.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "en",
                            "fileReplacements": [
                                {
                                    "replace": "src/frontend/environments/environment.ts",
                                    "with": "src/frontend/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "none",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false
                        }
                    }
                }
            }
        },
        "frontend-e2e": {
            "root": "src/e2e",
            "sourceRoot": "src/e2e",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "./frontend-protractor.conf.js",
                        "devServerTarget": "frontend:serve"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "e2e/tsconfig.e2e.json"
                        ],
                        "exclude": []
                    }
                }
            }
        }
    },
    "defaultProject": "frontend",
    "schematics": {
        "@schematics/angular:component": {
            "prefix": "app",
            "styleext": "css"
        },
        "@schematics/angular:directive": {
            "prefix": "app"
        }
    }
}
